<%#
# To change this template, choose Tools | Templates
# and open the template in the editor.
%>
<div style="background-image : url('../../images/room_floor_background.png'); width : 100%; height : 400px;">
<% for place in @places %>
<% if place.number == 2%>
 <div style="position : absolute; left : <%=place.x %>px; top : <%=place.y %>px"><img src="../../images/2_place.png" /></div>
<% else %>
 <div style="position : absolute; left : <%=place.x %>px; top : <%=place.y %>px"><img src="../../images/4_place.png" /></div>
<% end %>
<% end %>

</div>
